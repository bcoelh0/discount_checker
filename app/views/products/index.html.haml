%p#notice= notice
%h1 Products
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - @products.each do |product|
      %tr
        %td= link_to product.url, product.url, target: :blank
        %td=# link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' }

%br
= link_to "Home", root_path
